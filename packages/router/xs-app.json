{
    "welcomeFile": "approuterhome/index.html",
    "authenticationMethod": "none",
    "routes": [
        {
            "source": "^/approuterhome/(.*)$",
            "target": "$1",
            "localDir": "../ui-listofbooks/webapp",
            "cacheControl": "no-cache, no-store, must-revalidate"
        },
        {
            "source": "^/([^/]+)/(.*)$",
            "target": "$2",
            "destination": "$1"
        }
    ]
}
